{
	"name": "Spaceland Monokai (Light)",
	"type": "light",
	"colors": {
		"editor.background": "#f8f8f2",
		"editor.foreground": "#494949",
		"editor.lineHighlightBackground": "#d6d6cf",
		"editor.selectionBackground": "#b9b9af",
		"editorCursor.foreground": "#2e2e2e",
		"editorIndentGuide.activeBackground": "#b8630dbd",
    "editorWhitespace.foreground": "#3b3a32",
    "editorIndentGuide.background": "#b1b1aa",
    "editorLineNumber.foreground": "#b1b1aa",
    "editorLineNumber.activeForeground": "#494949"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
        "comment",
        "punctuation.definition.comment"
      ],
			"settings": {
				"foreground": "#a09b85"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#beb453"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#743fb1"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#743fb1"
			}
		},
		{
			"name": "User-defined constant",
			"scope": "constant.character, constant.other",
			"settings": {
				"foreground": "#743fb1"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#A32D64"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#A32D64"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#288299"
			}
		},
		{
			"name": "Class name",
			"scope": "entity.name.class",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#b96a0f"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "",
				"foreground": "#A32D64"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"fontStyle": "",
				"foreground": "#547c05"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#288299"
			}
		},
		{
			"name": "Library constant",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#288299"
			}
		},
		{
			"name": "Library class/type",
			"scope": "support.type, support.class",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#288299"
			}
		},
		{
			"name": "Library variable",
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"fontStyle": "",
				"foreground": "#F8F8F0"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#F8F8F0"
			}
		},
		{
			"name": "JSON String",
			"scope": "meta.structure.dictionary.json string.quoted.double.json",
			"settings": {
				"foreground": "#CFCFC2"
			}
		},
		{
			"name": "diff.header",
			"scope": "meta.diff, meta.diff.header",
			"settings": {
				"foreground": "#75715E"
			}
		},
		{
			"name": "diff.deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#A32D64"
			}
		},
		{
			"name": "diff.inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "diff.changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#547c05"
			}
		},
		{
			"name": "GitGutter deleted",
			"scope": "markup.deleted.git_gutter",
			"settings": {
				"foreground": "#A32D64"
			}
		},
		{
			"name": "GitGutter inserted",
			"scope": "markup.inserted.git_gutter",
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "GitGutter changed",
			"scope": "markup.changed.git_gutter",
			"settings": {
				"foreground": "#547c05"
			}
		},
		{
			"name": "GitGutter ignored",
			"scope": "markup.ignored.git_gutter",
			"settings": {
				"foreground": "#75715E"
			}
		},
		{
			"name": "GitGutter untracked",
			"scope": "markup.untracked.git_gutter",
			"settings": {
				"foreground": "#75715E"
			}
		},
		{
			"scope": "constant.numeric.line-number.find-in-files - match",
			"settings": {
				"foreground": "#743fb1A0"
			}
		},
		{
			"scope": "entity.name.filename.find-in-files",
			"settings": {
				"foreground": "#547c05"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#aa9708"
			}
		},
		{
			"scope": "keyword - keyword.operator",
			"settings": {
				"foreground": "#288299"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#288299"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#288299"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#288299"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#743fb1"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#a32d64"
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#a32d64"
			}
		},
		{
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#a32d64"
			}
		},
		{
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#a79e5c"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#8d8d8d",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"variable",
				"variable.other",
				"variable.parameter.function"
			],
			"settings": {
				"foreground": "#494949"
			}
		},
		{
			"scope": [
				"punctuation.terminator.statement.js",
				"punctuation.terminator.rule.css"
			],
			"settings": {
				"foreground": "#a32d64"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#b267e6"
			}
		}
	]
}
